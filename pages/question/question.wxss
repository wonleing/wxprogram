/* myself.wxss */
.box{
  width: auto;
  height: 100%;
  padding: 36.23rpx 44.08rpx; 
  overflow-y: scroll;
  padding-bottom: 150rpx;
  position: relative;
}
.list{
  margin-bottom: 54.54rpx;
  display: flex;
}
.list .list-num{
  flex: 1;
  font-size: 34.38rpx;
  font-weight: bold;
}
.list .content{
  flex: 15;
  color: #333;
}
.list .question{
  font-size: 34.38rpx;
  font-weight: bold;
  margin-bottom: 21.4rpx;
}
.list .answer{
  font-size: 27.17rpx;
  line-height: 36.23rpx;
}
