/* myself.wxss */
 .container{
  width: 694.6rpx;
  height: 100%;
  padding-left: 27.7rpx;
  padding-right: 27.7rpx;
  overflow-y: scroll;
  display: block;
}
.userinfo{
  display: flex;
}
.userName{
  position: absolute;
  left: 50.72rpx;
  line-height: 140.1rpx;
}
.userName .userinfo-nickname{
  display: inline-block;
  width: 300rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.userImage{
  position: absolute;
  right: 50.72rpx;
}
.userinfo-avatar {
  flex: 1;
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  position: relative;
  top: -80px;
  right: 0;
}

.userinfo-nickname {
  flex: 1;
  position: relative;
  top: -80px;
  left: 0;
  color: #040404;
  font-size: 46.5rpx;
  font-weight: bold;
}
.orderlist{
  position: absolute;
  left: 50.72rpx;
  right: 50.72rpx;
}
.orderlist .listName{
  width: 100%;
  font-size: 32.61rpx;
  line-height: 163.65rpx;
  border-bottom: 1px solid #bebdbd;
  position: relative;
}
.orderlist .listName:last-child{
  border-bottom: none;
}
.orderlist .listName .iconfont{
  position: absolute;
  right: 5%;
  top: 0;
  color: #b3b3b3;
  font-size: 56rpx;
  line-height: 163.65rpx;
}
.orderlist .listName button{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  color: #fff;
  font-size: 32.61rpx;
  text-align: left;
  background-color: #fff;
  line-height: 163.65rpx;
}
.orderlist .listName button::after{
   border: 0px;
}
.orderlist .listName .text{
  position: absolute;
  left: 0;
  z-index: 10;
}
