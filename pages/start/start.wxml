<view class="containers">
  <image animation="{{animationData}}" class='banner' src='{{bootimg.pic}}' style='left: {{leftValue}}rpx;'></image>
  <view class='titleIcon'>
    <image mode='widthFix' src='{{bootimg.topIcon}}'></image>
  </view>
  <view class="start" bindtap='startProject'>{{buttonText}}</view>
    <view class="typeIcon">
      <image mode='widthFix' src='{{bootimg.bottomIcon}}'></image>
    </view>
</view>
