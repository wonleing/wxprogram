.containers {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  overflow: hidden;
}
.banner{
  position: absolute;
  left: 50%;
  width: 200%;
  height: 100%;
  transition: all 1s;
}
.titleIcon{
  position: fixed;
  left: 24%;
  top: 10%;
  width: 52%;
  z-index: 10;
}
image{
  width: 100%;
}
.start{
  position: fixed;
  left: 30%;
  bottom: 13%;
  width: 40%;
  color: #1cac1b;
  font-size: 34rpx;
  text-align: center;
  letter-spacing: 6rpx;
  line-height: 82.12rpx;
  border-radius: 100rpx;
  background-color: #fff;
}
.typeIcon{
  position: fixed;
  left: 33%;
  bottom: 8%;
  width: 34%;
}
page{
  width: 100%;
  height: 100%;
}