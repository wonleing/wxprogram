/* experiene.wxss */
image{
  width: 100%;
  height: auto;
  border-radius: 10rpx;
}
.containers{
  width: auto;
  height: 100%;
  overflow-y: scroll;
}
 .swiper-container{
  position: relative;
  height: 821.26rpx;
  padding-left: 27.7rpx;
  padding-right: 27.7rpx;
}
.swiper-container .swiper-wrapper{
  height: 100%;
}
.swiper-container .swiper-wrapper .swiper-item{
  height: 100%;
  background-color: #f3f3f3;
}
.swiper-wrapper .wx-swiper-dots.wx-swiper-dots-horizontal{
  margin-bottom: 2rpx;
}
.swiper-wrapper .wx-swiper-dot{
  width: 50rpx;
  display: inline-flex;
  height: 6rpx;
  margin-left: 20rpx;
  justify-content: space-between;
}
.swiper-wrapper .wx-swiper-dot::before{
  content: '';
  flex-grow: 1;
  background: #9d9d9e;
  border-radius: 8rpx;
}
.swiper-wrapper .wx-swiper-dot-active::before{
  background: #fff;
}
.swiper-wrapper .swiper-item .slide-image{
  height: 100%;
  max-height: 821.26rpx;
  border-radius: 10rpx;
}
 .swiper-container .title{
  position: absolute;
  left: 0;
  top: 36.23rpx;
  padding-left: 23.55rpx;
  padding-right: 42.27rpx;
  line-height: 74.54rpx;
  font-size: 36.83rpx;
  font-weight: bold;
  text-align: center;
  background-color: #fff;
  border-top-right-radius: 60rpx;
  border-bottom-right-radius: 60rpx;
}
 .swiper-container .title .mapIcon{
  width: 48rpx;
  height: 48rpx;
  color: #4cc04b;
  font-size: 68rpx;
  font-weight: lighter;
  margin-right: 10rpx;
  vertical-align: sub;
  display: inline-block;
  margin-left: 23.55rpx;
}
 .swiper-container .title .mapIcon image{
   width: 100%;
   height: 100%;
   max-height: 821.26rpx;
 }
 .swiper-container .title text{
  vertical-align: top;
}
 .swiper-container .tool{
  position: absolute;
  right: 33.21rpx;
  top: 13.21rpx;
  color: #fff;
  font-size: 120rpx;
  padding: 0 6rpx;
  background-color: #4cc04b;
  border-radius: 50%;
  transform: scale(0.6);
  vertical-align: sub;
}
.swiper-wrapper .swiper-item .introduce{
  position: absolute;
  bottom: 79.71rpx;
  left: 51.93rpx;
  right: 51.93rpx;
  color: #eee;
  font-size: 25rpx;
  font-weight: lighter;
  line-height: 36.23rpx;
  letter-spacing: 1rpx;
}
.hot-recommend{
  font-size: 37.43rpx;
  font-weight: bold;
  line-height: 103.26rpx;
  padding-left: 45.89rpx;
  margin-top: 27.78rpx;
}
 .box{
  margin-bottom: 30rpx;
  overflow-x: scroll;
  padding-left: 27.7rpx;
}
.box .scroll-view_H{
  height: 770rpx;
  display: flex;
  white-space: nowrap;
  border-radius: 10rpx;
}
.box .scroll-view_H .image{
  width: 664.25rpx;
  height: 100%;
  margin-right: 24.75rpx;
  border: 1px solid #dbdbdb;
  border-radius: 10rpx;
  display: inline-block;
  vertical-align: top;
  box-sizing: border-box;
}
.box .scroll-view_H .recommend{
  height: 100%; 
  border-radius: 10rpx;
  position: relative;
  transition: all 1s;
}
.box .scroll-view_H .recommend .imgs{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.bj{
  background: #f3f3f3;
}
.box .scroll-view_H .recommend .heart{
  position: absolute;
  right: 33.21rpx;
  top: 33.21rpx;
  color: #fff;
  font-size: 48rpx;
  font-weight: bold;
  z-index: 100;
}
.box .scroll-view_H .recommend .collect{
  position: absolute;
  right: 33.21rpx;
  top: 33.21rpx;
  color: red;
  font-size: 48rpx;
  font-weight: bold;
  z-index: 100;
}
.box .scroll-view_H .recommend .banner{
  height: 585rpx;
  position: relative;
  background-color: #f3f3f3;
}
.box .scroll-view_H .recommend .banner image{
  height: auto;
  max-height: 585rpx;
  min-height: 585rpx;
}
.logo-gray{
  width: 20%;
  height: 18%;
  margin-left: 40%;
  margin-top: 39%;
}
.logo-gray-childs{
  width: 18%;
  height: 20%;
  margin-left: 39%;
  margin-top: 40%;
}
.box .scroll-view_H .recommend .popular-products{
  position: absolute;
  left: 0;
  top: 48.31rpx;
  width: 177.54rpx;
  color: #eee;
  font-size: 24.76rpx;
  text-align: center;
  letter-spacing: 3rpx;
  line-height: 54.35rpx;
  background-color: #4cc04b;
  border-top-right-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
}
.box .scroll-view_H .recommend .price{
  position: absolute;
  bottom: 37.44rpx;
  color: #fff;
  font-size: 45rpx;
  text-align: center;
  line-height: 94.80rpx;
  background-color: rgba(0, 0, 0, 0.8);
  padding-left: 31.4rpx;
  padding-right: 31.4rpx;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.box .scroll-view_H .message{
  padding-left: 35.63rpx;
  padding-right: 35.63rpx;
}
.box .scroll-view_H .message .tit{
  width: 95%;
  font-size: 35.02rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-top: 20rpx;
}
.box .scroll-view_H .message .count{
  color: #9c9c9c;
  font-size: 23rpx;
  line-height: 48.31rpx;
  display: flex;
}
.box .scroll-view_H .message text{
  letter-spacing: 3rpx;
  margin-right: 1rpx;
}
.box .scroll-view_H .message .count .star{
  display: flex;
}
.box .scroll-view_H .message .count .iconfont{
  color: #ff500b;
  font-size: 21.7rpx;
}
.person{
  margin-left: 4rpx;
}
 .distance{
  width: 100%;
  color: #9c9c9c;
  font-size: 26rpx;
  position: relative;
  margin-top: 15rpx;
}
 .distance .distanceIcon{
  font-size: 26rpx;
  font-weight: bold;
}
 .distance .meters{
  position: absolute;
  right: 0;
  display: flex;
}
 .distance .meters .meter{
  color: #08b096;
  margin-left: 21.13rpx;
}
.types{
  position: absolute;
  left: 0;
  top: 0;
}
.type{
  color: #52cf5e;
  font-size: 18.11rpx;
  border: 1px solid #52cf5e;
  border-radius: 10rpx;
  padding: 4rpx 12.08rpx;
  margin-right: 20rpx;
}
.type:nth-child(2){
  color: #ff0006;
  border: 1px solid #ff0006;
  margin-left: 10rpx;
}

.feature{
  margin-top: 10rpx;
}
.feature .scenic{
  font-size: 37.43rpx;
  font-weight: bold;
  line-height: 103.86rpx;
  padding-left: 45.89rpx;
}
.show{
  width: auto;
  text-align: center;
}
.show .showAll{
  color: #6bd0c0;
  font-size: 33.21rpx;
  line-height: 100rpx;
  padding-bottom: 10.87rpx;
  border-bottom: 4.83rpx solid #6bd0c0;
}
.active{
  padding-left: 27.7rpx;
  padding-right: 27.7rpx;
}
.pay-card{
  color: #fff;
  border-radius: 10rpx;
  padding: 21.74rpx;
  margin-bottom: 67.02rpx;
}
.pay-card .leyou{
  width: 100%;
  height: 270.53rpx;
  border-radius: 10rpx;
}