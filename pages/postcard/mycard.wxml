<view class='containers'>
  <scroll-view class='scroll-view' scroll-x>
    <view class='collect' wx:for='{{collect}}' wx:key='{{item}}' bindtap='carddetail' data-fr='{{item.fr}}' data-t='{{item.t}}' wx:if='{{item.postcardLink}}' data-content='{{item.detail}}' data-pic='{{item.postcardLink}}' data-mark='{{item.postmark}}'>
      <image src='{{item.postcardLink}}' mode='widthFix' lazy-load='true'></image>
    </view>
  </scroll-view>
</view>
