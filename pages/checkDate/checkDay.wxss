 .containers{
  height: 100%;
  overflow-y: scroll;
}
.altTitle{
  line-height: 120rpx;
  margin-left: 40rpx;
}
.box{
  padding: 10rpx;
  margin-bottom: 20rpx;
}
.title{
  font-size: 36rpx;
  font-weight: bold;
  margin-left: 40rpx;
  margin-bottom: 10rpx;
}
.week{
  display: flex;
  justify-content: center;
  align-items: center;
  line-height: 88rpx;
  border-bottom: 1px solid #cecece;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.week .weeks{
  width: 14%;
  font-size: 30rpx;
  text-align: center;
  font-weight: bold;
}
.date{
  display: flex;
  flex-wrap: wrap;
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin-top: 10rpx;
}
.date .day{
  width: 14%;
  line-height: 80rpx;
  font-size: 30rpx;
  text-align: center;
  margin-bottom: 20rpx;
}
.days{
  width:80rpx;
  height:80rpx;
  line-height: 80rpx;
  display: inline-block;
}
.choosed{
  width:80rpx;
  height:80rpx;
  background-color:#4ccabc;
  color:#fff;
  border-radius:100%;
  display: inline-block;
}