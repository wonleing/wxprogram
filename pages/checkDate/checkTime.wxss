.containers{
  width: 694.6rpx;
  height: 100%;
  padding-left: 30.2rpx;
  padding-right: 30.2rpx;
  overflow-y: scroll;
} 
.title{
  font-size: 32rpx;
  margin-bottom: 45.29rpx;
  margin-top: 42.27rpx;
}
.time{
  color: #999;
  font-size: 21.74rpx;
  margin-bottom: 39.25rpx;
}
.time-node{
  width: 100%;
  box-sizing: border-box;
}
.time-node .chooseTime{
  display: inline-block;
  width: 30%;
  letter-spacing: 4rpx;
  font-size: 28rpx;
  text-align: center;
  height: 78.5rpx;
  border: 1px solid #ff7502;
  border-radius: 40rpx;
  margin-bottom: 24.15rpx;
  margin-right: 2.5%;
  position: relative;
}
.time-node .active{
  color: #fff;
  background-color: #ff7502;
}
.time-node .chooseTime .man{
  position: absolute;
  right: 0;
  top: -4rpx;
  color: #ff7502;
  font-size: 24rpx;
}
.time-node .disabledChoose{
  border: none;
  color: #999;
}
